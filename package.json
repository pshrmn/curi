    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hickory/in-memory": "^2.0.0-beta.11",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/qs": "^6.5.2",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
