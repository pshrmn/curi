{
  "name": "@curi/svelte",
  "version": "2.0.0-beta.0",
  "description": "Curi integration with Svelte",
  "main": "dist/curi-svelte.js",
  "module": "dist/curi-svelte.es.js",
  "files": [
    "dist",
    "components",
    "LICENSE",
    "*.md"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist && rimraf components",
    "build": "BABEL_ENV=build rollup -c && node ./scripts/copyHTML",
    "prepare": "npm run build",
    "test": "npm run test:source && npm run test:cjs",
    "test:cjs": "TEST_ENV=cjs jest",
    "test:source": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/curi/tree/master/packages/svelte"
  },
  "keywords": [
    "curi",
    "svelte",
    "router"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "peerDependencies": {
    "@curi/router": ">2.0.0-beta.15"
  },
  "dependencies": {
    "svelte": "^3.0.0"
  }
}
