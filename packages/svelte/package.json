{
  "name": "@curi/svelte",
  "version": "1.0.0-beta.12",
  "description": "Curi integration with Svelte",
  "main": "dist/curi-svelte.js",
  "module": "dist/curi-svelte.es.js",
  "files": [
    "dist",
    "components",
    "LICENSE",
    "*.md"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist && rimraf components",
    "build": "BABEL_ENV=build rollup -c && node ./scripts/copyHTML",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "test": "jest && TEST_ENV=cjs jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/curi/tree/master/packages/svelte"
  },
  "keywords": [
    "curi",
    "svelte",
    "router"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "peerDependencies": {
    "@curi/router": "^1.1.0"
  },
  "dependencies": {
    "svelte": "^2.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@curi/router": "^1.1.0",
    "@hickory/in-memory": "^1.0.0",
    "babel-core": "bridge",
    "babel-jest": "^23.6.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-svelte": "^4.5.0",
    "simulant": "^0.2.2",
    "svelte-cli": "^3.0.0"
  }
}
