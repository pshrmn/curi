  "author": "Paul Sherman",
  "license": "MIT",
  "peerDependencies": {
    "@curi/core": "^1.0.0-beta.26"
  },
  "dependencies": {
    "svelte": ">=1.49.0"
  },
  "devDependencies": {
    "@curi/core": "^1.0.0-beta.29",
    "@hickory/in-memory": "^1.0.0-beta.4",
    "babel-core": "7.0.0-beta.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-external-helpers": "^7.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.0",
    "babel-preset-env": "^2.0.0-beta.0",
    "jest": "^22.1.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
