{
  "name": "@curi/addon-prefetch",
  "version": "1.0.0-beta.4",
  "description": "An addon to enable prefetching curi routes",
  "main": "dist/curi-addon-prefetch.common.js",
  "module": "dist/curi-addon-prefetch.es.js",
  "typse": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "*.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist & rimraf types",
    "build": "node ./scripts/build",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "prettier": "prettier --single-quote --write \"{src,tests}/**/*.js\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/curi/tree/master/packages/addons/addon-prefetch"
  },
  "keywords": [
    "curi",
    "prefetch",
    "load",
    "addon"
  ],
  "author": "Paul Sherman",
  "license": "MIT",
  "dependencies": {
    "@curi/core": "^1.0.0-beta.6",
    "@hickory/root": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.29",
    "jest": "^21.1.0",
    "prettier": "^1.7.0",
    "pretty-bytes": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "globals": {
      "ts-jest": {
        "module": "es6"
      }
    }
  }
}
