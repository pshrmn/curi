    "@types/node": "^8.0.53",
    "@vue/test-utils": "^1.0.0-beta.10",
    "jest": "^22.1.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-typescript2": "^0.14.0",
