{
  "name": "@curi/docs-website",
  "private": true,
  "version": "0.1.16",
  "description": "The documentation website for the Curi router",
  "scripts": {
    "prebuild": "npm run setupBuild && npm run versions && webpack-cli --config webpack.config.prod.js",
    "build": "npm run generate",
    "dev": "cross-env BABEL_ENV=dev webpack-dev-server --config webpack.config.dev.js",
    "generate": "cross-env BABEL_ENV=build node ./scripts/build",
    "prettier": "prettier --single-quote --write \"src/**/*.js\"",
    "setupBuild": "node ./scripts/buildSetup",
    "versions": "node ./scripts/updatePackageVersions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/curi/tree/gh-pages"
  },
  "author": "Paul Sherman",
  "license": "UNLICENSED",
  "dependencies": {
    "@curi/react-dom": "^1.1.0",
    "@curi/route-active": "^1.0.1",
    "@curi/router": "^1.0.3",
    "@curi/side-effect-aria-live": "^1.0.0",
    "@curi/side-effect-scroll": "^1.0.0",
    "@curi/side-effect-title": "^1.0.0",
    "@curi/static": "^1.0.0-alpha.3",
    "@hickory/browser": "^1.0.0",
    "@hickory/in-memory": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^7.1.6",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "fs-extra": "^4.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "path-to-regexp": "^2.2.0",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "request-promise-native": "^1.0.5",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.8"
  }
}
