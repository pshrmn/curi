{
  "name": "@curi-docs/website",
  "private": true,
  "version": "0.2.6",
  "description": "The documentation website for the Curi router",
  "scripts": {
    "prebuild": "npm run build:setup && npm run versions && npm run build:bundle",
    "build:bundle": "webpack-cli --config webpack.config.prod.js",
    "build:setup": "node ./scripts/buildSetup",
    "build": "cross-env BABEL_ENV=node NODE_ENV=production node ./scripts/build",
    "predev": "node ./scripts/generateDevIndexHtml",
    "dev": "cross-env BABEL_ENV=dev webpack-dev-server --config webpack.config.dev.js",
    "prettier": "prettier --single-quote --write \"src/**/*.js\"",
    "versions": "node ./scripts/updatePackageVersions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshrmn/curi/tree/gh-pages"
  },
  "author": "Paul Sherman",
  "license": "UNLICENSED",
  "dependencies": {
    "@curi/helpers": "file:../packages/helpers",
    "@curi/react-dom": "file:../packages/react-dom",
    "@curi/route-prefetch": "file:../packages/interactions/route-prefetch",
    "@curi/router": "file:../packages/router",
    "@curi/side-effect-aria-live": "file:../packages/side-effects/side-effect-aria-live",
    "@curi/side-effect-scroll": "file:../packages/side-effects/side-effect-scroll",
    "@curi/side-effect-title": "file:../packages/side-effects/side-effect-title",
    "@curi/static": "file:../packages/static",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@hickory/browser": "^2.0.0-beta.11",
    "@hickory/in-memory": "^2.0.0-beta.11",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0"
  }
}
